<div class="container">
  <h2>Results index</h2>
  
  <table class="table table-hover table-inverse">
    <% if @range == "User" %>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
  
      <tbody>
        <%= render 'users/index', users: @users %>
      </tbody>
  
    <% elsif @range == "Book" || @range == 'Tag' %>
  
   
      <thead>
        <tr>
          <th></th>
          <th>Title</th>
          <th>Opinion</th>
          <th colspan="3"></th>
        </tr>
      </thead>
  
      <tbody>
        <%= render 'books/index' ,books: @books %>
      </tbody>
    <% end %>
  </table>
</div>